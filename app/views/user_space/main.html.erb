<head>
  <meta charset="utf-8">
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="icon" type="image/png" href="favicon.ico">

  <!--Google Font link-->
  <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Raleway:400,600,700" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'vendor/modernizr-2.8.3-respond-1.4.2.min.js', 'data-turbolinks-track': 'reload' %>
</head>

<body data-spy="scroll" data-target=".navbar-collapse">


<div class="culmn">
  <!--Home page style-->


  <nav class="navbar navbar-default navbar-fixed green no-background bootsnav">

    <div class="container">

      <!-- Start Header Navigation -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-menu">
          <i class="fa fa-bars"></i>
        </button>
        <a class="navbar-brand" href="#brand">

          <img class="logo logo-display m-top-10" <%= image_tag "logo.png" %>
          <img class="logo logo-scrolled"  <%= image_tag "logo.png" %>

        </a>
      </div>
      <!-- End Header Navigation -->

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="navbar-menu">
        <ul class="nav navbar-nav navbar-right blue" data-in="fadeInDown" data-out="fadeOutUp">
          <li> <%= link_to "RETOUR ACCUEIL", root_path %>
          </li>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div>


  </nav>


  <!--Home Sections (id hello home bg-mega) -->
  <section class="background-cover slick">
    <!--    <div class="overlay"></div>-->
    <div class="container slide slide--1">
      <div class="row bck_layer">
        <div class="main_home col-md-12">
          <div>
            <h1 class="home_text">Espace administrateur</h1>
          </div>
        </div>
        <div class="main_home col-md-12">
          <%= link_to "Tableau de bord", admin_users_path, class: "btn btn-outline-secondary" %>
          <%= link_to "Gérer les albums", picture_path, class: "btn btn-outline-secondary" %>
        </div>
      </div><!--End off row-->
    </div><!--End off container -->
    <div class="container slide slide--2">
      <div class="row bck_layer">
        <div class="main_home col-md-6">
          <div>
            <h1 class="home_text">Bienvenue <br/> sur notre site</h1>
          </div>
        </div>
        <div class="main_home col-md-6">
          <h3 class="slide_text text-justify"> Qu’est ce que la marche nordique </h3>
          <h6 class="slide_text text-justify"> « Plus dynamique que la randonnée, la marche nordique a pour principe
            d’accentuer le mouvement de balancier naturel des bras à l’aide de deux bâtons. <br/>Le corps est ainsi
            propulsé vers l’avant. On va plus vite, plus loin avec moins de fatigue. <br/>
            La dépense d’énergie est accrue et le plaisir de la pratique arrive des les premières foulées car la
            technique est simple. <br/>
            Accessible à tous, cette pratique ravira les jeunes, les moins jeunes, quelle que soit leur condition
            physique. <br/>C’est aussi un sport familial et convivial qui regroupe un large public »
          </h6>
        </div>
      </div><!--End off row-->
    </div><!--End off container -->
  </section> <!--End off Home Sections-->
</div><!--End off container -->
</section><!--End off Contact Section-->


<footer id="footer" class="footer bg-black">
  <div class="container">
    <div class="row">
      <div class="main_footer text-center p-top-40 p-bottom-30">
        <p class="wow fadeInRight" data-wow-duration="1s">
          Made with
          <i class="fa fa-heart"></i>
          by
          <a target="_blank" href="https://github.com/lucaslamy">Lucas LAMY</a>
          2019. All Rights Reserved
        </p>
      </div>
    </div>
  </div>
</footer>


</div>


<script>
    $('.slick').slick({
        autoplay: true,
        autoplaySpeed: 2000,
        fade: true,
        arrows: false
    });

    function showmap() {
        var myLatLng = {lat: 49.347289, lng: 2.977947};
        var mapOptions = {
            zoom: 18,
            scrollwheel: false,
            center: myLatLng,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        var map = new google.maps.Map(document.getElementById('map_canvas'), mapOptions);
        $('.mapheight').css('height', '350');
        $('.maps_text h3').hide();
        var marker = new google.maps.Marker({
            position: myLatLng,
            map: map,
            title: 'VitalMarche'
        });
    }

</script>

<script src="http://maps.google.com/maps/api/js?key=AIzaSyD_tAQD36pKp9v4at5AnpGbvBUsLCOSJx8"></script>
<script src="https://kit.fontawesome.com/30d548dfb8.js" crossorigin="anonymous"></script>
